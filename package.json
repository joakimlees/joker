{
  "name": "joker",
  "version": "0.1.1",
  "description": "Application that tells jokes",
  "type": "module",
  "main": "src/index.js",
  "scripts": {
    "test": "node src/js/joker.test.js",
    "format": "prettier -w src/**/*.js",
    "lint": "eslint src/**/*.js",
    "lint-fix": "eslint src/**/*.js --cache --fix",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joakimlees/joker.git"
  },
  "keywords": [
    "workflow",
    "git"
  ],
  "author": "Joakim Lee Sletten <joakimlee_sletten@outlook.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/joakimlees/joker/issues"
  },
  "homepage": "https://github.com/joakimlees/joker#readme",
  "devDependencies": {
    "eslint": "^8.27.0",
    "husky": "^8.0.2",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1"
  },
  "dependencies": {
    "give-me-a-joke": "^0.5.0"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.html": [
      "prettier --write"
    ],
    "*.scss": [
      "prettier --write"
    ]
  }
}
